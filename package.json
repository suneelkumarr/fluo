{
  "name": "fluo",
  "version": "1.1.0",
  "description": "Ultra-fast, feature-rich terminal styling",
  "keywords": [
    "terminal",
    "console",
    "cli",
    "colors",
    "ansi",
    "styles",
    "chalk",
    "colorette",
    "picocolors",
    "gradient",
    "rainbow",
    "256-colors",
    "truecolor",
    "rgb",
    "hex",
    "fast",
    "lightweight"
  ],
  "author": "Suneel Kumar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuneelKumarr/fluo.git"
  },
  "homepage": "https://github.com/SuneelKumarr/fluo#readme",
  "bugs": {
    "url": "https://github.com/SuneelKumarr/fluo/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": "{\"import\": \"./dist/index.js\", \"require\": \"./dist/index.cjs\"}",
  "files": "[\"dist\"]",
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c && npm run minify",
    "minify": "terser dist/index.mjs -o dist/index.min.mjs -c -m",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "node benchmark/bench.js",
    "bench:compare": "node benchmark/compare.js",
    "bench:memory": "node --expose-gc benchmark/memory.js",
    "bench:startup": "node benchmark/startup.js",
    "bench:all": "npm run bench && npm run bench:compare && npm run bench:memory && npm run bench:startup",
    "bench:json": "node benchmark/compare.js --json > benchmark/results.json",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "size": "size-limit",
    "docs": "typedoc src/index.ts --out docs"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/node": "^20.10.0",
    "chalk": "^5.3.0",
    "colorette": "^2.0.20",
    "kleur": "^4.1.5",
    "mitata": "^0.1.6",
    "picocolors": "^1.0.0",
    "rollup": "^4.6.0",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "terser": "^5.24.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0",
    "yoctocolors": "^2.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "3 kB"
    },
    {
      "path": "dist/core.mjs",
      "limit": "1.5 kB"
    }
  ],
  "directories": {
    "test": "test"
  }
}
